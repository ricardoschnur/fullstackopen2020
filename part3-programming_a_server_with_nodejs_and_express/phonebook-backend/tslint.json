{
  "defaultSeverity": "error",
  "extends": ["tslint-config-airbnb"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "quotemark": true,
    "variable-name": {
      "options": "allow-leading-underscore"
    },
    "trailing-comma": false,
    "max-line-length": {
      "options": { "limit": 180 }
    }
  },
  "rulesDirectory": []
}
